<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Flag</th>
        </tr>
    </thead>
    <tbody>
        {{#data}}<tr>
            <td>{{code}}</td>
            <td>{{name}}</td>
            <td><img class="thumbnail" src="{{BASE_URI}}assets/img/lib/flags/{{code}}.png" /></td>
        </tr>{{/data}}
    </tbody>
</table>
